{
	"name": "bit3/contao-magickimages",
	"description": "ImageMagick integration for Contao OpenSource CMS",
	"keywords": ["contao", "images", "imagemagick"],
	"type": "contao-module",
	"homepage": "http://bit3.de",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Tristan Lins",
			"email": "tristan.lins@bit3.de",
			"homepage": "http://bit3.de",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@bit3.de",
		"issues": "https://github.com/bit3/contao-magickimages/issues",
		"wiki": "http://de.contaowiki.org/MagickImages",
		"irc": "irc://irc.freenode.org/contao.dev",
		"source": "https://github.com/bit3/contao-magickimages"
	},
	"require": {
		"php": ">=5.3",
		"contao/core": "3.1.*",
		"contao-community-alliance/composer-installer": "*",
		"bit3/contao-dependency-container": "~1",
		"symfony/process": "2.3.*"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "1.*",
		"bit3/php-coding-standard": "@dev"
	},
	"replace": {
		"contao-legacy/magickimages": "*"
	},
	"autoload": {
		"psr-0": {
			"MagickImages": "src/"
		}
	},
	"extra": {
		"contao": {
			"symlinks": {
				"contao": "system/modules/theme-plus"
			}
		}
	}
}
